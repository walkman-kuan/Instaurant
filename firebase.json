{
  "database": {
      "rules": "./database-rules.json"
  },
  "storage": {
      "rules": "./storage.rules"
  },
  "hosting": {
    "public": "./",
    "ignore": [
      "firebase.json",
      "./database-rules.json",
      "./storage.rules",
      ".*",
      "dataModels/**",
      "src/**",
      "package.json",
      "README.md",
      "webpack.config.js"
    ],
    "rewrites": [ {
      "source": "**",
      "destination": "/index.html"
    } ]
  }
}
