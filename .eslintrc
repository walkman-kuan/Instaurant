{
    "env": {
      "browser": true
    },
    "extends": "airbnb",
    "rules": {
        "indent": [2, 4],
        "max-len": ["warn", 120],
        "react/forbid-prop-types": [1, {"forbid": ["any"]}],
        "react/jsx-indent": ["warn", 4],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}
